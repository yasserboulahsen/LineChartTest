                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </VBox.margin></Button>
            <Button fx:id="curseur" mnemonicParsing="false" onAction="#onCursor" prefHeight="25.0" prefWidth="97.0" text="Curseur">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <bottom>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="start" mnemonicParsing="false" onAction="#onStart" prefHeight="27.0" prefWidth="100.0" text="Start" textFill="#1500ff">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onStop" prefHeight="26.0" prefWidth="99.0" text="Stop" textFill="RED">
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <top>
      <HBox alignment="CENTER" prefHeight="27.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets left="5.0" right="5.0" />
         </opaqueInsets>
         <children>
            <Button fx:id="connect" mnemonicParsing="false" onAction="#onConnexion" prefHeight="26.0" prefWidth="153.0" text="connexion">
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
   </top>
   <right>
      <VBox alignment="CENTER" prefHeight="336.0" prefWidth="91.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane alignment="TOP_CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label alignment="CENTER" prefHeight="27.0" prefWidth="42.0" text="X : " />
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="44.0" text="0" GridPane.columnIndex="1" fx:id="xValue" />
                  <Label alignment="CENTER" prefHeight="30.0" prefWidth="47.0" text="Y : " GridPane.rowIndex="1" />
                  <Label fx:id="yValue" accessibleHelp="valeur de y" alignment="CENTER" prefHeight="27.0" prefWidth="59.0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <GridPane alignment="BOTTOM_CENTER">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="47.0" text="X :" />
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="47.0" text="0" GridPane.columnIndex="1" fx:id="xValue2" />
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="47.0" text=" Y :" GridPane.rowIndex="1" />
                        <Label fx:id="yValue2" alignment="CENTER" prefHeight="24.0" prefWidth="47.0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </children>
      </VBox>
   </right>
